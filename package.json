    "@types/react-dom": "^18.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.5",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "uuid": "^9.0.0",
